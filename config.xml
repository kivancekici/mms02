<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mms.baklava7" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Deneme</name>
    <description>
        Deneme.De Mobil UygulamasÄ±
    </description>
    <author email="info@kivancekici.com" href="http://kivancekici.com">
        M.Sc. Computer Engineer Suleyman Kivanc Ekici
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon platform="android" qualifier="ldpi" src="images/icons/mipmap-mdpi/ic_launcher.png" />
    <icon platform="android" qualifier="mdpi" src="images/icons/mipmap-mdpi/ic_launcher.png" />
    <icon platform="android" qualifier="hdpi" src="images/icons/mipmap-hdpi/ic_launcher.png" />
    <icon platform="android" qualifier="xhdpi" src="images/icons/mipmap-xhdpi/ic_launcher.png" />
    <icon platform="android" qualifier="xxhdpi" src="images/icons/mipmap-xxhdpi/ic_launcher.png" />
    <icon platform="android" qualifier="xxxhdpi" src="images/icons/mipmap-xxxhdpi/ic_launcher.png" />
    <icon platform="android" qualifier="fr-xxhdpi" src="images/icons/mipmap-xxxhdpi/ic_launcher.png" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-console" spec="1.0.7" />
    <plugin name="cordova-plugin-statusbar" spec="1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="1.2.2" />
    <engine name="browser" spec="~5.0.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="~2.0.2" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.4" />
    <plugin name="com.threescreens.cordova.plugin.brotherPrinter.ql" spec="https://github.com/askinsaglam/cordova-brother-printer-ql.git" />
</widget>
